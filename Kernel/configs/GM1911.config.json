[
  {
    "kernelSource": {
      "name":"栩柠的PRTS",
      "repo": "https://github.com/LifeLikeLemon/android_kernel_oneplus_sm8150",
      "branch": "14.0",
      "device": "GM1911",
      "defconfig": "vendor/sm8150_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/crdroidandroid/android_prebuilts_clang_host_linux-x86_clang-7141649",
        "branch": "11.0",
        "name": "crdroid-clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": true
  }
]
